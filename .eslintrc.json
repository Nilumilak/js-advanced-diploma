{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": 0,
        "indent": 0,
        "quotes": 0,
        "class-methods-use-this": 0,
        "no-unused-vars": 0,
        "semi": 0,
        "no-plusplus": 0,
        "eqeqeq": 0,
        "max-len": 0,
        "arrow-parens": 0,
        "no-restricted-syntax": 0,
        "no-alert": 0,
        "no-trailing-spaces": 0,
        "no-use-before-define": 0
    }
}
